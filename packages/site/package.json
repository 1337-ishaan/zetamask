{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "license": "(MIT-0 OR Apache-2.0)",
  "exports": {
    "./helpers/evm": "./dist/helpers/evm.js"
  },
  "scripts": {
    "allow-scripts": "yarn workspace root allow-scripts",
    "build": "GATSBY_TELEMETRY_DISABLED=1 gatsby build",
    "clean": "rimraf public",
    "lint": "yarn lint:eslint && yarn lint:misc --check",
    "lint:eslint": "eslint . --cache --ext js,ts",
    "lint:fix": "yarn lint:eslint --fix && yarn lint:misc --write",
    "lint:misc": "prettier '**/*.json' '**/*.md' '!CHANGELOG.md' --ignore-path .gitignore",
    "start": "GATSBY_TELEMETRY_DISABLED=1 gatsby develop"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@metamask/providers": "^17.1.2",
    "@openzeppelin/contracts": "5.0.2",
    "@types/react-modal": "3.16.3",
    "bignumber.js": "^9.1.2",
    "crypto-js": "4.2.0",
    "decimal.js": "^10.4.3",
    "dompurify": "3.1.6",
    "ethers": "^6.13.2",
    "rc-tooltip": "6.2.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropdown": "1.11.0",
    "react-is": "18.2.0",
    "react-modal": "3.16.1",
    "react-qr-code": "2.0.12",
    "react-toastify": "10.0.5",
    "recharts": "2.12.7",
    "styled-components": "5.3.3",
    "zetalink": "workspace:^"
  },
  "devDependencies": {
    "@metamask/eslint-config": "^13.0.0",
    "@metamask/eslint-config-browser": "14.0.0",
    "@metamask/eslint-config-jest": "^13.0.0",
    "@metamask/eslint-config-nodejs": "^13.0.0",
    "@metamask/eslint-config-typescript": "^13.0.0",
    "@svgr/webpack": "6.4.0",
    "@testing-library/dom": "8.17.1",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "13.5.0",
    "@types/dompurify": "3.0.5",
    "@types/jest": "27.5.2",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "5.1.25",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.8.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-n": "^17.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^7.1.0",
    "gatsby": "^5.13.7",
    "gatsby-plugin-manifest": "^5.13.1",
    "gatsby-plugin-styled-components": "^6.13.1",
    "gatsby-plugin-svgr": "^3.0.0-beta.0",
    "prettier": "^3.3.3",
    "prettier-plugin-packagejson": "2.2.18",
    "react-accessible-accordion": "5.0.0",
    "rimraf": "3.0.2",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.18.0"
  }
